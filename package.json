{
  "name": "music-before-die",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "rewrites": "NODE_OPTIONS='--experimental-json-modules' node ./src/scripts/rewrites.mjs",
    "nextconfig": "npm run rewrites",
    "pre": "npm run nextconfig && next dev",
    "dev": "tinacms dev -c \"yarn pre\"",
    "prebuild": "tinacms build && npm run nextconfig",
    "build": "next build",
    "start": "tinacms build && npm run nextconfig && next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.2.32",
    "@tinacms/cli": "^1.5.11",
    "daisyui": "^2.51.6",
    "framer-motion": "^10.12.8",
    "next": "13.4.1",
    "next-intl": "2.15.0-beta.3",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-wrap-balancer": "^0.4.1",
    "slugify": "^1.6.6",
    "tinacms": "^1.5.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/negotiator": "^0.6.1",
    "@types/node": "20.0.0",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "10.4.14",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.39.0",
    "eslint-config-next": "13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "8.4.23",
    "prettier": "^2.8.8",
    "tailwindcss": "3.3.2",
    "tailwindcss-debug-screens": "^2.2.1",
    "tailwindcss-fluid-type": "^2.0.3",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "*.js,ts,tsx,": "eslint --cache --fix",
    "*.{js,ts,tsx,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
